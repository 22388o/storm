{
  "name": "@syvita/storm",
  "version": "0.1.0",
  "description": "CLI authenticator for Stacks CLI apps",
  "main": "index.js",
  "scripts": {
    "preinstall": "pnpm build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "esbuild index.js --bundle --platform=node --external:keytar --format=cjs --outdir=build",
    "start": "pnpm build && node build/index.js",
    "i2p": "pnpm build && pnpm i -g",
    "launch": "pnpm publish --access=public"
  },
  "author": "Asteria <asteria@syvita.org>",
  "license": "ISC",
  "dependencies": {
    "@noble/secp256k1": "^1.3.0",
    "@stacks/wallet-sdk": "^2.0.1",
    "bip39": "^3.0.4",
    "c32check": "^1.1.3",
    "chalk": "^4.1.2",
    "chalk-animation": "^1.6.0",
    "commander": "^8.3.0",
    "conf": "^10.1.0",
    "hash-wasm": "^4.9.0",
    "keytar": "^7.7.0",
    "micro-stacks": "^0.1.3",
    "node-fetch": "^3.1.0",
    "ora": "^6.0.1",
    "prompts": "^2.4.2",
    "ripemd160-js": "^1.2.0",
    "search-array": "^1.3.2",
    "string-crypto": "^2.0.2",
    "table": "^6.7.3",
    "which": "^2.0.2"
  },
  "bin": {
    "storm": "build/index.js"
  },
  "devDependencies": {
    "esbuild": "^0.13.15"
  }
}
